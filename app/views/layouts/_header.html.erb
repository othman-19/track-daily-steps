<header class="logoBar" >
  <div class="container">
    <h1 class="center" id="logo" >Track Daily Progress </h1>
      <% if user_signed_in? %>
        <h5 id="user" class="center"> <span class="userName"><%= current_user.name %></span>  <span class="sign-out-in"><%= link_to (fa_icon "sign-out", text: 'Log out'), destroy_user_session_path, method: :delete %></span>  </h5> 
      <% else %>
        <h5 id="user" class="center"> <span class="userName"><%= link_to (fa_icon "sign-in", text: "Log_in"), new_user_session_path %></span> <span class="sign-out-in"><%= link_to (fa_icon "sign-in", text: "Sign Up"), new_user_registration_path %></span></h5> 
      <% end %>
  </div>
</header>