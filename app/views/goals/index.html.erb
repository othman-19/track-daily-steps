<h1> user goals <h1>
<%= link_to current_user.name, current_user %>
<% for goal in @user_goals %>
  <%= goal.description =%> <br>
  <%= goal.start =%> <br>
  <%= goal.end =%> <br>
  <%= goal.startTime =%> <br>
  <%= goal.workingTime =%> <br>
  <%= goal.duration =%> <br>
  <%= goal.performance =%> <br>
  <%= link_to 'Goal', goal %>
<% end %>

